package renderer

templ IconImageTemplate(r *Renderer, p *IconImageRenderParams) {
	<div id={"block-" + p.Id } class={"block blockIconPage index0 noPlus " + p.Classes}>
		<div class="wrapContent">
			<div id="block-icon" class="iconObject c96 isPage withImage">
				<div class="iconEmoji" style="line-height: 56px;">
					<img src={ p.Src } class="iconImage c56" />
				</div>
			</div>
		</div>
	</div>
}
