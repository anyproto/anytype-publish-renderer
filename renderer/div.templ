package renderer

templ DivTemplate(p *DivRenderParams) {
	<div id={"block-" + p.Id } class={ p.Classes }>
		<div class="content">
			@p.Comp
		</div>
	</div>
}

templ DivDotTemplate() {
	<div class="dots">
		<div class="dot"></div>
		<div class="dot"></div>
		<div class="dot"></div>
	</div>
}

templ DivLineTemplate() {
	<div class="line"></div>
}
