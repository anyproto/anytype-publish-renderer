package renderer

templ PageTemplate(r *Renderer, p *RenderPageParams) {
	<!DOCTYPE html>
    <html lang="en">
        <head>
			<title>{ r.titleText() }</title>
			<link rel="apple-touch-icon" sizes="180x180" href="https://anytype.io/apple-touch-icon.png" />
			<link rel="icon" type="image/png" sizes="32x32" href="https://anytype.io/favicon-32x32.png" />
			<link rel="icon" type="image/png" sizes="16x16" href="https://anytype.io/favicon-16x16.png" />
            <link rel="stylesheet" href={ r.AssetResolver.GetStaticFolderUrl("/layout.css") } />
			<link rel="stylesheet" href={ r.AssetResolver.GetPrismJsUrl("/themes/prism.css") } />

			<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/katex@0.16.11/dist/katex.min.css" integrity="sha384-nB0miv6/jRmo5UMMR1wu3Gz6NLsoTkbqJghGIsx//Rlm+ZU03BU6SQNC66uf4l5+" crossorigin="anonymous" />

			<!-- The loading of KaTeX is deferred to speed up page rendering -->
			<script defer src="https://cdn.jsdelivr.net/npm/katex@0.16.11/dist/katex.min.js" integrity="sha384-7zkQWkzuo3B5mTepMUcHkMB5jZaolc2xDwL6VFqjFALcbeS9Ggm/Yr2r3Dy4lfFg" crossorigin="anonymous"></script>

        </head>
		<body>
			<script src={ r.AssetResolver.GetPrismJsUrl("/components/prism-core.min.js") }></script>
			<script src={ r.AssetResolver.GetPrismJsUrl("/plugins/autoloader/prism-autoloader.min.js") }></script>

			<main>
				if r.joinSpaceLink() != "" {
					<div class="joinSpace">
						<a href={r.joinSpaceLink()} target="blank">Join Space</a>
					</div>
				}
				@r.RenderPageCover()
				<div class={"blocks " + p.Classes }>
 					@r.RenderPageIconImage()
					<div>
						for _, childId := range r.Root.ChildrenIds {
							@r.RenderBlock(r.BlocksById[childId])
						}
					</div>
				</div>
				<a href="https://anytype.io/" target="blank">
 					<div class="madeInAnytype">
						<div class="container">
							<div class="anyIcon">
								<svg width="14" height="14" viewBox="0 0 14 14" fill="none" xmlns="http://www.w3.org/2000/svg">
									<path fill-rule="evenodd" clip-rule="evenodd" d="M11 3V0H3V3H11ZM11 3V14H14V3H11ZM4.5 14C6.98528 14 9 11.9853 9 9.5C9 7.01472 6.98528 5 4.5 5C2.01472 5 0 7.01472 0 9.5C0 11.9853 2.01472 14 4.5 14Z" fill="black"/>
								</svg>
							</div>
							<div class="madeInText">
								Made in Anytype
							</div>
						</div>
					</div>
				</a>

			</main>
			<script src={ r.AssetResolver.GetStaticFolderUrl("/scripts.js") }></script>

        </body>
    </html>
}
